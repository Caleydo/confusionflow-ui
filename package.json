{
  "private": true,
  "scripts": {
    "predist": "npm run build && npm run docs",
    "predocker": "npm run build",
    "docker": "docker build -t malevo -f deploy/Dockerfile .",
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/**.ts tests/**.ts",
    "docs": "sphinx-apidoc -o docs -f ./malevo_server && sphinx-build ./docs build/docs",
    "prebuild": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test",
    "pretest": "npm run check",
    "test": "test ! -d tests || python setup.py test",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack",
    "build": "python build.py",
    "start": "webpack-dev-server --inline",
    "start:hot": "webpack-dev-server --inline --hot",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "dist": "python setup.py bdist_egg && cd build && tar cvzf ../dist/malevo_server.tar.gz *",
    "check": "flake8"
  },
  "files": [
    "malevo_server",
    "__init__.py",
    "__main__.py",
    "build",
    "requirements.txt",
    "requirements_dev.txt",
    "docker_packages.txt"
  ],
  "engines": {
    "npm": ">= 3",
    "node": ">= 6"
  },
  "dependencies": {
    "phovea_core": "github:phovea/phovea_core#develop",
    "phovea_ui": "github:phovea/phovea_ui#develop",
    "@types/d3": "3.5.36",
    "d3": "3.5.17"
  },
  "name": "malevo_server",
  "description": "",
  "homepage": "https://phovea.caleydo.org",
  "version": "1.0.0-SNAPSHOT",
  "author": {
    "name": "Martin Ennemoser",
    "email": "martin.ennemoser@jku.at",
    "url": ""
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phovea/malevo_server/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caleydo/malevo.git"
  }
}
